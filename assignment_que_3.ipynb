    "series = [21,22,23,24,25,26,27,28,29]\n",
    "even_count=0\n",
    "odd_count=0\n",
    "for i in series:\n",
    "    if i%2==0:\n",
    "        even_count+=1\n",
    "        \n",
    "    else:\n",
    "        odd_count+=1\n",
    "            \n",
    "print(\"Even numbers in series :\", even_count)\n",
    "print(\"Odd numbers in series :\", odd_count)"
   ]
  }
